{
  "extends": "@augu/tsconfig/dom.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "exclude": ["node_modules"]
}
